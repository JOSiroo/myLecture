package com.day24;

class Person{
	public void display() {
		System.out.println("this = " + this);
	}
}

public class ObjectTest {

	public static void main(String[] args) {
		Person p = new Person();
		Person p2 = new Person();
		
		System.out.println("두 객체가 같은가?" + p.equals(p2));
		System.out.println("p객체의 클래스 ? " + p.getClass());
	
	}

}
