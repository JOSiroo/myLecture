package eeee;
import java.util.ArrayList;
import java.util.Collections;
import java.util.TreeSet;

public class PokerGame {
	public static void main(String[] args) {
		TreeSet<Integer> set = new TreeSet<>();
		while(set.size() < 6) {
			int num = (int)(Math.random()*45)+1;	//1~45
			set.add(num);
		}
	}
}
